<launch>
  <node pkg="stage" type="stageros" name="simulator" args="$(find GrassBoss)/world/GrassBoss.world"/>
  <node pkg="teleop_base" type="teleop_base_keyboard" name="keyboard" args="base_controller/command:=cmd_vel" output="screen" />
  <!--<node pkg="rviz" type="rviz" name="visualization" args="$(find GrassBoss)/conf/GrassBoss.vcg"/>-->

  <node pkg="GrassBoss" type="angle_parser" name="angle_parser">
    <param name="scanner_topic" value="/base_scan"/>
    <param name="angle_topic" value="/angle"/>
  </node>

  <node pkg="GrassBoss" name="moveConstantVelocity" type="moveConstantVelocity.py"/>
  <node pkg="GrassBoss" name="timed_turn" type="stupidCoverageBehaviour.py"/>
  <node pkg="smach_viewer" name="stateMachine_viewer" type="smach_viewer.py"/>
</launch>
